{
  "name": "react-algoliasearch-helper",
  "version": "1.1.0",
  "description": "React `<Provider helper>` and `connect()(WrappedComponent)` for algoliasearch-helper",
  "scripts": {
    "start": "webpack-dev-server --config webpack.config.dev.babel.js",
    "build": "./scripts/build.sh",
    "release": "./scripts/release.sh",
    "test": "jest --bail && npm run lint && npm run build",
    "test:watch": "jest --watch",
    "lint": "rm -rf dist/ && eslint ."
  },
  "repository": "algolia/react-algoliasearch-helper",
  "keywords": [
    "algolia",
    "react",
    "helper"
  ],
  "author": "Algolia (https://www.algolia.com)",
  "license": "MIT",
  "devDependencies": {
    "algoliasearch": "^3.17.0",
    "algoliasearch-helper": "^2.11.0",
    "babel-cli": "^6.10.1",
    "babel-core": "^6.11.4",
    "babel-jest": "^13.2.2",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-1": "^6.5.0",
    "component-playground": "^1.3.0",
    "conventional-changelog-cli": "^1.2.0",
    "doctoc": "^1.2.0",
    "enzyme": "^2.4.1",
    "eslint-config-algolia": "^5.3.1",
    "jest-cli": "^13.2.3",
    "json": "^9.0.4",
    "pretty-bytes-cli": "^1.0.0",
    "raw-loader": "^0.5.1",
    "react": "^15.2.1",
    "react-algoliasearch-helper": "file:./",
    "react-dom": "^15.2.1",
    "react-hot-loader": "^1.3.0",
    "uglify-js": "^2.7.0",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "react-addons-shallow-compare": "^15.2.1"
  },
  "peerDependencies": {
    "algoliasearch": "^3.17.0",
    "algoliasearch-helper": "^2.11.0",
    "react": "^15.2.1"
  },
  "jest": {
    "testRegex": "\\.test\\.js$",
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react/",
      "<rootDir>/node_modules/enzyme/",
      "<rootDir>/node_modules/react-addons-shallow-compare"
    ]
  }
}
